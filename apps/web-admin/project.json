{
  "name": "@oksai/web-admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web-admin/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web-admin",
        "command": "pnpm dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/.output"],
      "options": {
        "cwd": "apps/web-admin",
        "command": "pnpm build"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web-admin",
        "command": "pnpm start"
      }
    }
  },
  "tags": ["type:app", "scope:frontend", "framework:tanstack-start"]
}
